    glVertex3f(-65.7,28,-64.08);
    glVertex3f(-70.7,28,-49.4);
    glVertex3f(-75.7,28,-24.7);
    glVertex3f(-77.7,28,0);
    glVertex3f(-56.7,28,0);
    glEnd();
    glBegin(GL_POLYGON);//atap kiri depan
    glColor3ub(255,255,255);
    glVertex3f(-56.7,28,69);
    glVertex3f(-60.7,28,70.93);
    glVertex3f(-63.7,28,67.59);
    glVertex3f(-65.7,28,64.08);
    glVertex3f(-70.7,28,49.4);
    glVertex3f(-75.7,28,24.7);
    glVertex3f(-77.7,28,0);
    glVertex3f(-56.7,28,0);
    glEnd();
    glBegin(GL_POLYGON);//atap kanan blkng
    glColor3ub(255,255,255);
    glVertex3f(56.7,28,-69);
    glVertex3f(60.7,28,-70.93);
    glVertex3f(63.7,28,-67.59);
    glVertex3f(65.7,28,-64.08);
    glVertex3f(70.7,28,-49.4);
    glVertex3f(75.7,28,-24.7);
    glVertex3f(77.7,28,0);
    glVertex3f(56.7,28,0);
    glEnd();
    glBegin(GL_POLYGON);//atap kanan depan
    glColor3ub(255,255,255);
    glVertex3f(56.7,28,69);
    glVertex3f(60.7,28,70.93);
