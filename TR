    glVertex3f(-18.9,28,81.9);//blkng2
    glVertex3f(-37.8,28,78.9);//blkng1
    glVertex3f(-56.7,28,74.1);//pkr blkng1
    glVertex3f(-60.7,28,70.93);//pkr blng 2
    glVertex3f(-62.7,28,69.09);//pkr blkng 3
    glEnd();
    glBegin(GL_POLYGON);//atap depan kanan
    glColor3ub(255,255,255);
    glVertex3f(0.0,28,69.09);//ubah z max tribun
