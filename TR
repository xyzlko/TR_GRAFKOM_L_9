    glVertex3f(-46.7,34,69);
    glVertex3f(-46.7,29,69);
    glEnd();
    glBegin(GL_LINE_LOOP);//lis layar papan score krd
    glColor3ub(80,80,80);
    glVertex3f(- 56.7,29,59);
    glVertex3f(-56.7,34,59);
    glVertex3f(-46.7,34,69);
    glVertex3f(-46.7,29,69);
    glEnd();
    glEnd();
    glBegin(GL_POLYGON);//layar papan score knb
    glColor3ub(125, 255, 231);
    glVertex3f(56.7,29,-59);
